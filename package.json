{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "npm run generate:css -- --minify",
    "build:gql": "graphql-codegen",
    "build:remix": "remix build",
    "dev": "run-p \"dev:*\"",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "generate:css": "npx tailwindcss -i ./app/styles/app.css -o ./app/__generated__/tailwind.css",
    "start": "remix-serve build",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@heroicons/react": "^1.0.6",
    "@remix-run/node": "^1.10.0",
    "@remix-run/react": "^1.10.0",
    "@remix-run/serve": "^1.10.0",
    "@tailwindcss/typography": "^0.5.9",
    "@tanstack/react-query": "^4.24.6",
    "boring-avatars": "^1.7.0",
    "chart.js": "^4.2.1",
    "chartjs-adapter-luxon": "^1.3.1",
    "graphql": "^16.6.0",
    "graphql-request": "^5.1.0",
    "graphql-tag": "^2.12.6",
    "isbot": "^3.6.5",
    "luxon": "^3.2.1",
    "ms": "^2.1.3",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-ssr-prepass": "^1.5.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.16.5",
    "@graphql-codegen/client-preset": "^2.0.0",
    "@graphql-codegen/typed-document-node": "^3.0.0",
    "@graphql-codegen/typescript": "^2.8.2",
    "@graphql-codegen/typescript-graphql-request": "^4.5.8",
    "@graphql-codegen/typescript-operations": "^2.5.7",
    "@graphql-typed-document-node/core": "^3.1.1",
    "@remix-run/dev": "^1.10.0",
    "@remix-run/eslint-config": "^1.10.0",
    "@types/chart.js": "^2.9.37",
    "@types/luxon": "^3.2.0",
    "@types/ms": "^0.7.31",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "eslint": "^8.27.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=14"
  }
}
